  <ItemGroup>
    <PackageReference Include="AsyncBridge" Version="0.3.1" />
    <PackageReference Include="SD.Common" Version="3.1.2" />
    <PackageReference Include="SD.IOC.Core" Version="1.1.0" />
    <PackageReference Include="SD.Infrastructure" Version="3.4.2" />
