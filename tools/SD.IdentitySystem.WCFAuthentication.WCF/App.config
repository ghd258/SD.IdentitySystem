<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!--缓存提供者配置节点-->
    <section name="cacheProviderConfiguration" type="SD.CacheManager.CacheProviderConfiguration,SD.CacheManager" />
  </configSections>
  <system.serviceModel>
    <!--WCF行为配置-->
    <behaviors>
      <serviceBehaviors>
        <behavior name="DefaultBehavior">
          <!--WCF客户端/服务端身份认证扩展配置-->
          <wcfAuthentication />
        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
        <behavior name="DefaultBehavior">
          <!--WCF客户端/服务端身份认证扩展配置-->
          <wcfAuthentication />
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <!--WCF扩展配置-->
    <extensions>
      <behaviorExtensions>
        <!--WCF客户端/服务端身份认证扩展配置-->
        <add name="wcfAuthentication" type="SD.IdentitySystem.WCFAuthentication.WCF.AuthenticationBehaviorElement, SD.IdentitySystem.WCFAuthentication.WCF" />
      </behaviorExtensions>
    </extensions>
  </system.serviceModel>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Configuration.ConfigurationManager" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.1.0" newVersion="4.0.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>